<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chillcoding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_quiz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_quiz_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloYou">
        <activity
            android:name=".BottomNavigationActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuizActivity"
            android:exported="false" />
        <activity
            android:name=".QuizTransportActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"/>
    </application>

</manifest>